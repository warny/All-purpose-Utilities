<?xml version="1.0" encoding="utf-8" ?>
<Numbers xmlns="Utils/NumberConvertionConfiguration.xsd">
        <Language
                groupSize="3"
                separator=" "
                groupSeparator=""
                zero="cero"
                minus="menos *"
                decimalSeparator="coma"
                fractionSeparator="sobre"
        >
                <Culture>GL</Culture>
                <Culture>gl-ES</Culture>
                <Groups>
                        <Group level="1">
                                <Digit digit="0" string="" />
                                <Digit digit="1" string="un" />
                                <Digit digit="2" string="dous" />
                                <Digit digit="3" string="tres" />
                                <Digit digit="4" string="catro" />
                                <Digit digit="5" string="cinco" />
                                <Digit digit="6" string="seis" />
                                <Digit digit="7" string="sete" />
                                <Digit digit="8" string="oito" />
                                <Digit digit="9" string="nove" />
                        </Group>
                        <Group level="2">
                                <Digit digit="0" string="" buildString="*" />
                                <Digit digit="1" string="dez" buildString="dez *" />
                                <Digit digit="2" string="vinte" buildString="vinte e *" />
                                <Digit digit="3" string="trinta" buildString="trinta e *" />
                                <Digit digit="4" string="corenta" buildString="corenta e *" />
                                <Digit digit="5" string="cincuenta" buildString="cincuenta e *" />
                                <Digit digit="6" string="sesenta" buildString="sesenta e *" />
                                <Digit digit="7" string="setenta" buildString="setenta e *" />
                                <Digit digit="8" string="oitenta" buildString="oitenta e *" />
                                <Digit digit="9" string="noventa" buildString="noventa e *" />
                        </Group>
                        <Group level="3">
                                <Digit digit="0" string="" buildString="*" />
                                <Digit digit="1" string="cen" buildString="cento *" />
                                <Digit digit="2" string="douscentos" buildString="douscentos *" />
                                <Digit digit="3" string="trescentos" buildString="trescentos *" />
                                <Digit digit="4" string="catrocentos" buildString="catrocentos *" />
                                <Digit digit="5" string="cincocentos" buildString="cincocentos *" />
                                <Digit digit="6" string="seiscentos" buildString="seiscentos *" />
                                <Digit digit="7" string="setecentos" buildString="setecentos *" />
                                <Digit digit="8" string="oitocentos" buildString="oitocentos *" />
                                <Digit digit="9" string="novecentos" buildString="novecentos *" />
                        </Group>
                </Groups>
                <NumberScale firstLetterUpperCase="false">
                        <StaticNames>
                                <Scale value="0" string="" />
                                <Scale value="1" string="mil" />
                                <Scale value="2" string="mill(ón|óns)" />
                                <Scale value="3" string="mil mill(ón|óns)" />
                        </StaticNames>
                        <Suffixes>
                                <Suffix>(ón|óns)</Suffix>
                        </Suffixes>
                </NumberScale>
                <Replacements>
                        <Replacement oldValue="un mil" newValue="mil" />
                </Replacements>
                <Exceptions>
                        <Number value="11" string="once" />
                        <Number value="12" string="doce" />
                        <Number value="13" string="trece" />
                        <Number value="14" string="catorce" />
                        <Number value="15" string="quince" />
                        <Number value="16" string="dezaseis" />
                        <Number value="17" string="dezasete" />
                        <Number value="18" string="dezaoito" />
                        <Number value="19" string="dezanove" />
                </Exceptions>
        </Language>
</Numbers>
