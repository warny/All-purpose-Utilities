<?xml version="1.0" encoding="utf-8" ?>
<Numbers xmlns="Utils/NumberConvertionConfiguration.xsd">
	<Language
        groupSize="3"
        separator=" "
        groupSeparator=""
        zero="zero"
        minus="meno *"
    >
		<Culture>IT</Culture>
		<Groups>
			<Group level="1">
				<Digit digit="0" string="" />
				<Digit digit="1" string="uno" />
				<Digit digit="2" string="due" />
				<Digit digit="3" string="tre" />
				<Digit digit="4" string="quattro" />
				<Digit digit="5" string="cinque" />
				<Digit digit="6" string="sei" />
				<Digit digit="7" string="sette" />
				<Digit digit="8" string="otto" />
				<Digit digit="9" string="nove" />
			</Group>
			<Group level="2">
				<Digit digit="0" string="" buildString="*" />
				<Digit digit="1" string="dieci" buildString="dici*" />
				<Digit digit="2" string="venti" buildString="venti*" />
				<Digit digit="3" string="trenta" buildString="trenta*" />
				<Digit digit="4" string="quaranta" buildString="quaranta*" />
				<Digit digit="5" string="cinquanta" buildString="cinquanta*" />
				<Digit digit="6" string="sessanta" buildString="sessanta*" />
				<Digit digit="7" string="settanta" buildString="settanta*" />
				<Digit digit="8" string="ottanta" buildString="ottanta*" />
				<Digit digit="9" string="novanta" buildString="novanta*" />
			</Group>
			<Group level="3">
				<Digit digit="0" string="" buildString="*" />
				<Digit digit="1" string="cento" buildString="cento*" />
				<Digit digit="2" string="duecento" buildString="duecento*" />
				<Digit digit="3" string="trecento" buildString="trecento*" />
				<Digit digit="4" string="quattrocento" buildString="quattrocento*" />
				<Digit digit="5" string="cinquecento" buildString="cinquecento*" />
				<Digit digit="6" string="seicento" buildString="seicento*" />
				<Digit digit="7" string="settecento" buildString="settecento*" />
				<Digit digit="8" string="ottocento" buildString="ottocento*" />
				<Digit digit="9" string="novecento" buildString="novecento*" />
			</Group>
		</Groups>
		<NumberScale firstLetterUpperCase="true">
			<StaticNames>
				<Scale value="0" string="" />
				<Scale value="1" string="mi(lle|la)" />
			</StaticNames>
			<Suffixes>
				<Suffix>on(e|i)</Suffix>
				<Suffix>ard(o|i)</Suffix>
			</Suffixes>
		</NumberScale>
		<Replacements>
			<Replacement oldValue="uno mille" newValue="mille" />
		</Replacements>
		<Exceptions>
			<Exceptions>
				<Number value="11" string="undici" />
				<Number value="12" string="dodici" />
				<Number value="13" string="tredici" />
				<Number value="14" string="quattordici" />
				<Number value="15" string="quindici" />
				<Number value="16" string="sedici" />
				<Number value="17" string="diciassette" />
				<Number value="18" string="diciotto" />
				<Number value="19" string="diciannove" />
			</Exceptions>
		</Exceptions>
	</Language>
</Numbers>