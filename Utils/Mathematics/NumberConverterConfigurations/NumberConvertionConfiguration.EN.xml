<?xml version="1.0" encoding="utf-8" ?>
<Numbers xmlns="Utils/NumberConvertionConfiguration.xsd">
	<Language
			groupSize="3"
			separator=" "
			groupSeparator=","
			zero="zero"
			minus="minus *"
			decimalSeparator="point"
        >
		<Culture>EN</Culture>
		<Culture>EN-uk</Culture>
		<Culture>EN-us</Culture>
		<Groups>
			<Group level="1">
				<Digit digit="0" string=""  />
				<Digit digit="1" string="one" />
				<Digit digit="2" string="two" />
				<Digit digit="3" string="three" />
				<Digit digit="4" string="four" />
				<Digit digit="5" string="five" />
				<Digit digit="6" string="six"  />
				<Digit digit="7" string="seven"  />
				<Digit digit="8" string="eight" />
				<Digit digit="9" string="nine" />
			</Group>
			<Group level="2">
				<Digit digit="0" string=""  buildString="*"  />
				<Digit digit="1" string="ten" buildString="ten *" />
				<Digit digit="2" string="twenty" buildString="twenty-*" />
				<Digit digit="3" string="thirty" buildString="thirty-*" />
				<Digit digit="4" string="forty" buildString="forty-*" />
				<Digit digit="5" string="fifty" buildString="fifty-*" />
				<Digit digit="6" string="sixty" buildString="sixty-*" />
				<Digit digit="7" string="seventy" buildString="seventy-*" />
				<Digit digit="8" string="eighty" buildString="eighty-*" />
				<Digit digit="9" string="ninety" buildString="ninety-*" />
			</Group>
			<Group level="3">
				<Digit digit="0" string="" buildString="*" />
				<Digit digit="1" string="one hundred" buildString="one hundred and *" />
				<Digit digit="2" string="two hundred" buildString="two hundred and *" />
				<Digit digit="3" string="three hundred" buildString="three hundred and *" />
				<Digit digit="4" string="four hundred" buildString="four hundred and *" />
				<Digit digit="5" string="five hundred" buildString="five hundred and *" />
				<Digit digit="6" string="six hundred" buildString="six hundred and *" />
				<Digit digit="7" string="seven hundred" buildString="seven hundred and *" />
				<Digit digit="8" string="eight hundred" buildString="eight hundred and *" />
				<Digit digit="9" string="nine hundred" buildString="nine hundred and *" />
			</Group>
		</Groups>
		<NumberScale firstLetterUpperCase="false">
			<StaticNames>
				<Scale value="0" string=""/>
				<Scale value="1" string="thousand"/>
			</StaticNames>
			<Suffixes>
				<Suffix>on</Suffix>
			</Suffixes>
		</NumberScale>
		<Exceptions>
			<Number value="11" string="eleven" />
			<Number value="12" string="twelve" />
			<Number value="13" string="thirteen" />
			<Number value="14" string="fourteen" />
			<Number value="15" string="fifteen" />
			<Number value="16" string="sixteen" />
			<Number value="17" string="seventeen" />
			<Number value="18" string="eighteen" />
			<Number value="19" string="nineteen" />
		</Exceptions>
		<Fractions>
			<Fraction digits="1" string="tenth(s)" />
			<Fraction digits="2" string="hundredth(s)" />
			<Fraction digits="3" string="thousandth(s)" />
		</Fractions>
	</Language>
</Numbers>